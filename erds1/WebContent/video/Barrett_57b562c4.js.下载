function BarrettMu(e){this.modulus=biCopy(e),this.k=biHighIndex(this.modulus)+1;var t=new BigInt;t.digits[2*this.k]=1,this.mu=biDivide(t,this.modulus),this.bkplus1=new BigInt,this.bkplus1.digits[this.k+1]=1,this.modulo=BarrettMu_modulo,this.multiplyMod=BarrettMu_multiplyMod,this.powMod=BarrettMu_powMod}function BarrettMu_modulo(e){var t=biDivideByRadixPower(e,this.k-1),a=biMultiply(t,this.mu),i=biDivideByRadixPower(a,this.k+1),n=biModuloByRadixPower(e,this.k+1),o=biMultiply(i,this.modulus),l=biModuloByRadixPower(o,this.k+1),r=biSubtract(n,l);r.isNeg&&(r=biAdd(r,this.bkplus1));for(var s=biCompare(r,this.modulus)>=0;s;)r=biSubtract(r,this.modulus),s=biCompare(r,this.modulus)>=0;return r}function BarrettMu_multiplyMod(e,t){var a=biMultiply(e,t);return this.modulo(a)}function BarrettMu_powMod(e,t){var a=new BigInt;a.digits[0]=1;for(var i=e,n=t;;){if(0!=(1&n.digits[0])&&(a=this.multiplyMod(a,i)),n=biShiftRight(n,1),0==n.digits[0]&&0==biHighIndex(n))break;i=this.multiplyMod(i,i)}return a}